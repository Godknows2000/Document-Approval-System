@using DocumentApprovalSystem.Lib
@{
    Layout = "_AdminLayout";
    switch (User.GetUserType())
    {
        case UserType.EMPLOYEE:
            ViewBag.TopNavPath = "_Employee";
            break;
        case UserType.TECH_SUPPORT:
            ViewBag.TopNavPath = "_Admin";
            //ViewBag.SideNavPath = "/Pages/Shared/_Nav.cshtml";
            ViewBag.SideNavPath = "~/Areas/Application/Pages/_Nav.cshtml";
            break;
        default:
            ViewBag.TopNavPath = "_Default";
            break;
    }

}
